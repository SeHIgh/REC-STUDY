import java.util.Scanner;
public class Report_02_01 {
	public static void main(String[]args) {
		Scanner scanner = Scanner(System.in);
		int score;
		char credit;
		System.out.println("성적을 입력하시오: ");
		score = scanner.nextInt();
		switch(score) {
		case 100: case 90:
			credit = 'A';
			break;
		case 80:
			credit = 'B';
			break;
		case 70:
			credit = 'C';
			break;
		case 60:
			credit = 'D';
			break;
		default :
			credit = 'F';
		}
		System.out.println("학점: "+credit);
	}
}
