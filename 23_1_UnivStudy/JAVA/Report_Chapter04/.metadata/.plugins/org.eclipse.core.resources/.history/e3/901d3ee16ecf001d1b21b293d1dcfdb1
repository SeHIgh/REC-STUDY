public class Account1 extends Account {
	private String name;
	private double balance;
	
	public Account1(String name){
		this.name = name;
		this.balance = 0.0;
	}
	
	public static void main(String[]args) {
		Account1 me;
		me = new Account1("장세현");
		System.out.println("고객님의 이름은 "+me.name+" 입니다.");
	}
}